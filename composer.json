{
  "name": "ariasbros/eloquent-locale",
  "description": "A package to make Eloquent models localizables",
  "keywords": [
    "ariasbros",
    "eloquent-locale",
    "laravel",
    "eloquent",
    "translation",
    "translate",
    "model",
    "l10n",
    "localization",
    "l12y",
    "localizability",
    "multilingual"
  ],
  "homepage": "https://github.com/AriasBros/eloquent-locale",
  "license": "MIT",
  "authors": [
    {
      "name": "David Arias VÃ¡zquez",
      "email": "david@ariasbros.com",
      "homepage": "https://www.ariasbros.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/database": "~5.6.0",
    "illuminate/support": "~5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "~3.5.0",
    "mockery/mockery": "^0.9.4",
    "php-coveralls/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Locale\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Locale\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Locale\\ServiceProvider"
      ]
    }
  }
}
